<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.assistant.mapper.DepartmentMapper">

    <select id="selectAll" resultType="com.assistant.model.enity.Department">
        select *
        from department
    </select>
    <select id="count" resultType="java.lang.Long">
        select count(*)
        from department
    </select>
</mapper>